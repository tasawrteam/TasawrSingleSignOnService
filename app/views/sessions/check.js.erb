var sessionId = '<%= u cookies[request.session_options[:key]] %>';
var sessionKey = '<%= request.session_options[:key] %>';

var TasawrAuth = {
  init: function(options) {
    <% if logged_in? %>
      if (options != null && options['ifLoggedIn'] != null) {
        var url = options['ifLoggedIn'];
        if (url.indexOf('?') != -1) {
          url += '&sessionId=' + sessionId + '&sessionKey=' + sessionKey;
        } else {
          url += '?sessionId=' + sessionId + '&sessionKey=' + sessionKey;
        }
        window.location = url;
      }
    <% else %>
      if (options != null && options['ifLoggedOut'] != null) {
        window.location = options['ifLoggedOut'];
      }
    <% end %>
  }
}